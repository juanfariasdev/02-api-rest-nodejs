{
  "name": "api-rest-nodejs",
  "version": "1.0.0",
  "description": "Api Rest using nodeJS and fastify",
  "main": "server.ts",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "tsup src --out-dir build",
    "dev": "tsx watch src/server.ts",
    "knex": "node --no-warnings --loader tsx ./node_modules/knex/bin/cli.js",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest"
  },
  "author": "Juan Pablo Farias (juanfarias.dev)",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@rocketseat/eslint-config": "^1.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "fastify": "^4.18.0",
    "knex": "^2.4.2",
    "pg": "^8.11.1",
    "prettier": "^2.8.8",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@types/supertest": "^2.0.12",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "vitest": "^0.32.4"
  }
}
